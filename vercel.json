{
  "rewrites": [
    { "source": "/admin", "destination": "/" },
    { "source": "/admin/", "destination": "/" },
    { "source": "/admin/:match*", "destination": "/" },
    { "source": "/:match*", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/(.*).mp4",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "video/mp4"
        }
      ]
    },
    {
      "source": "/(.*).png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
} 